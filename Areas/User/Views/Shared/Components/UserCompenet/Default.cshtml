@model List<User>

<table class="table table-hover table-striped">
    <thead>
        <tr>
            <td>ID</td>
            <td>List Number</td>
            <td>FullName</td>
            <td>Created Date</td>
            <td>Update Date</td>
            <td>Update</td>
            <td>Delete</td>
        </tr>
    </thead>
    <tbody>
        @foreach (User item in Model)
        {
            <tr>
                <td>@item.ListNumber</td>
                <td>@item.id</td>
                <td>@String.Join(" ", item.Name, item.Lastname)</td>
                <td>@item.CreatedDate</td>
                <td>@item.UpdateDate</td>
                <td>
                    <a asp-action="Update" asp-controller="User" asp-route-id="@item.id" class="btn btn-warning">Update</a>
                </td>
                <td>
                    <a asp-action="Delete" asp-controller="User" asp-route-id="@item.id" class="btn btn-danger">Delete</a>
                </td>
            </tr>
        }
    </tbody>
</table>